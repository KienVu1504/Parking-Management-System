<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="HistoryPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="720.0" minWidth="1280.0" prefHeight="768.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HistoryController">
   <children>
      <MenuBar fx:id="menuBar" prefWidth="1360.0">
         <menus>
            <Menu id="Menu" mnemonicParsing="false" text="Menu">
               <items>
                  <MenuItem id="In" mnemonicParsing="false" onAction="#goToIn" text="In">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="I" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem id="History" mnemonicParsing="false" onAction="#goToOut" text="Out">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="O" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem id="Close" mnemonicParsing="false" onAction="#closeAPP" text="Close">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="C" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
               </items>
            </Menu>
            <Menu id="Admin" mnemonicParsing="false" text="Admin">
               <items>
                  <MenuItem id="LogIn" mnemonicParsing="false" onAction="#goToAdmin" text="Login">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="L" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
               </items>
            </Menu>
            <Menu id="Help" mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem id="Tutorials" mnemonicParsing="false" onAction="#goToHelp" text="Tutorials">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="T" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem id="About" mnemonicParsing="false" onAction="#goToAbout" text="About">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="A" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Label alignment="CENTER" layoutY="26.0" prefHeight="54.0" prefWidth="1360.0" text="History">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <TableView fx:id="historyTable" layoutY="80.0" prefHeight="688.0" prefWidth="1360.0">
        <columns>
          <TableColumn id="Numbers" fx:id="numbersColumn" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="Numbers" />
          <TableColumn id="License Plate" fx:id="licensePlateColumn" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" text="License Plate" />
            <TableColumn id="Vehicle Type" fx:id="vehicleTypeColumn" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Vehicle Type" />
            <TableColumn id="Monthly Ticket" fx:id="monthlyTicketColumn" maxWidth="110.0" minWidth="110.0" prefWidth="110.0" text="Monthly Ticket" />
            <TableColumn id="Time In" fx:id="timeInColumn" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" text="Time In" />
            <TableColumn id="Time Out" fx:id="timeOutColumn" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" text="Time Out" />
            <TableColumn id="Parking Fee" fx:id="parkingFeeColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Parking Fee" />
            <TableColumn id="Status" fx:id="statusColumn" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" text="Status" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
