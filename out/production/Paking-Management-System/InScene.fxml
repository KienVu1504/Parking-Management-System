<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="InPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="720.0" minWidth="1280.0" prefHeight="768.0" prefWidth="1360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InController">
   <children>
      <MenuBar prefWidth="1360.0">
        <menus>
          <Menu id="Menu" mnemonicParsing="false" text="Menu">
            <items>
              <MenuItem id="Out" mnemonicParsing="false" onAction="#goToOut" text="Out" />
                  <MenuItem id="History" mnemonicParsing="false" onAction="#goToHistory" text="History" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem id="Close" mnemonicParsing="false" onAction="#closeAPP" text="Close" />
            </items>
          </Menu>
          <Menu id="Admin" mnemonicParsing="false" text="Admin">
            <items>
              <MenuItem id="LogIn" mnemonicParsing="false" onAction="#goToAdmin" text="Login" />
            </items>
          </Menu>
          <Menu id="Help" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem id="Tutorials" mnemonicParsing="false" onAction="#goToHelp" text="Tutorials" />
                  <MenuItem id="About" mnemonicParsing="false" onAction="#goToAbout" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label alignment="CENTER" layoutY="120.0" prefWidth="1360.0" text="Car In">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label layoutX="350.0" layoutY="250.0" text="License Plate">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="350.0" layoutY="530.0" text="Time In">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="350.0" layoutY="320.0" text="Vehicle Type">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="350.0" layoutY="460.0" text="Monthly Ticket">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="licensePlateTextField" layoutX="510.0" layoutY="250.0" prefWidth="500.0" />
      <TextField fx:id="timeInField" disable="true" editable="false" layoutX="510.0" layoutY="530.0" prefHeight="26.0" prefWidth="420.0" />
      <RadioButton fx:id="vehicleBicycles" layoutX="510.0" layoutY="320.0" mnemonicParsing="false" onAction="#carTypeChecked" text="Bicycles">
         <font>
            <Font size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="vehicleType" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="vehicleTypeCar" layoutX="950.0" layoutY="320.0" mnemonicParsing="false" onAction="#carTypeChecked" text="Car" toggleGroup="$vehicleType">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="vehicleTypeMotorbike" layoutX="702.0" layoutY="320.0" mnemonicParsing="false" onAction="#carTypeChecked" selected="true" text="Motorbike" toggleGroup="$vehicleType">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="monthlyTicketYes" layoutX="620.0" layoutY="460.0" mnemonicParsing="false" text="Yes">
         <font>
            <Font size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="ticket" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="monthlyTicketNo" layoutX="857.0" layoutY="460.0" mnemonicParsing="false" selected="true" text="No" toggleGroup="$ticket">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
      <Button layoutX="640.0" layoutY="648.0" mnemonicParsing="false" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="carSeatsLabel" disable="true" layoutX="350.0" layoutY="390.0" text="Car Seats">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="carSeats1" disable="true" layoutX="510.0" layoutY="390.0" mnemonicParsing="false" selected="true" text="4 - 8">
         <font>
            <Font size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="carSeats" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="carSeats3" disable="true" layoutX="950.0" layoutY="390.0" mnemonicParsing="false" text="30+" toggleGroup="$carSeats">
